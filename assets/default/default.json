{
BitmapFont: {
	comical_cartoon: {
		file: comical_cartoon.fnt
		scaledSize: -1
		markupEnabled: false
		flip: false
	}
	font-export: {
		file: font-export.fnt
		scaledSize: -1
		markupEnabled: false
		flip: false
	}
	super_comic: {
		file: super_comic.fnt
		scaledSize: -1
		markupEnabled: false
		flip: false
	}
}
Color: {
	Ash: {
		hex: 737373ff
	}
	Black: {
		hex: 000000ff
	}
	Cyan: {
		hex: b5fcffff
	}
	White: {
		hex: ffffffff
	}
}
TintedDrawable: {
	cursor: {
		name: white-pixel
		color: Black
		minWidth: 1
		minHeight: 30
	}
}
ImageTextButtonStyle: {
	default: {
		imageUp: sword
		font: super_comic
